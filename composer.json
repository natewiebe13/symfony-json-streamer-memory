{
    "name": "natewiebe13/symfony-json-streamer-memory",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "symfony/json-streamer": "7.3.*",
        "phpstan/phpdoc-parser": "^2.2",
        "halaxa/json-machine": "^1.2",
        "cweagans/composer-patches": "2.0.0-beta2"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3"
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "patches": {
            "symfony/json-streamer": {
                "Support iterating over arrays without casting": "./pr-61561.patch"
            }
        }
    }
}
